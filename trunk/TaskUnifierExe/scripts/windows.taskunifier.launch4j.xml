<launch4jConfig>
  <dontWrapJar>true</dontWrapJar>
  <headerType>gui</headerType>
  <jar>..\temp\TaskUnifier\TaskUnifier.jar</jar>
  <outfile>..\temp\TaskUnifier\TaskUnifier.exe</outfile>
  <errTitle></errTitle>
  <cmdLine></cmdLine>
  <chdir></chdir>
  <priority>normal</priority>
  <downloadUrl>http://java.com/download</downloadUrl>
  <supportUrl></supportUrl>
  <customProcName>false</customProcName>
  <stayAlive>false</stayAlive>
  <manifest></manifest>
  <icon>..\temp\TaskUnifier\resources\icon.ico</icon>
  <jre>
    <path></path>
    <minVersion>1.6.0</minVersion>
    <maxVersion></maxVersion>
    <jdkPreference>preferJre</jdkPreference>
    <maxHeapSize>512</maxHeapSize>
    <opt>-Dcom.leclercb.taskunifier.resource_folder=resources</opt>
    <opt>-Dcom.leclercb.taskunifier.data_folder=%APPDATA%/TaskUnifier</opt>
  </jre>
</launch4jConfig>