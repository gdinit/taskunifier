<?xml version="1.0" encoding="ISO-8859-1"?>
<launch4jConfig>
	<dontWrapJar>true</dontWrapJar>
	<headerType>gui</headerType>
	<jar>TaskUnifier.jar</jar>
	<outfile>../temp/TaskUnifier/TaskUnifier.exe</outfile>
	<errTitle>TaskUnifier</errTitle>
	<cmdLine></cmdLine>
	<chdir></chdir>
	<priority>normal</priority>
	<downloadUrl>http://java.com/download</downloadUrl>
	<supportUrl></supportUrl>
	<customProcName>false</customProcName>
	<stayAlive>false</stayAlive>
	<manifest></manifest>
	<icon>../temp/TaskUnifier/resources/icon.ico</icon>
	<jre>
		<path></path>
		<minVersion>1.6.0</minVersion>
		<maxVersion></maxVersion>
		<jdkPreference>preferJre</jdkPreference>
		<maxHeapSize>512</maxHeapSize>
		<opt>-splash:resources/images/splash_screen.png</opt>
		<opt>-Dcom.leclercb.taskunifier.resource_folder="resources"</opt>
		<opt>-Dcom.leclercb.taskunifier.data_folder="%APPDATA%/TaskUnifier"</opt>
	</jre>
	<versionInfo>
		<fileVersion>1.0.0.0</fileVersion>
		<txtFileVersion>1.0</txtFileVersion>
		<fileDescription>TaskUnifier Application</fileDescription>
		<copyright>2011, Benjamin Leclerc</copyright>
		<productVersion>1.0.0.0</productVersion>
		<txtProductVersion>1.0</txtProductVersion>
		<productName>TaskUnifier</productName>
		<companyName></companyName>
		<internalName>TaskUnifier</internalName>
		<originalFilename>TaskUnifier.exe</originalFilename>
	</versionInfo>
</launch4jConfig>